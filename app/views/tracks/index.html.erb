<h1>Tracks</h1>

<table class="table table-striped table-bordered">
  <thead class="thead-dark">
    <tr>
      <th>Track</th>
      <th>Album</th>
      <th>Artist</th>
    </tr>
  </thead>
  <tbody>
    <% @tracks.each do | track | %>
    <tr>
      <td><%= track.name %> </td>
      <td><%= track.album.title %></td>
      <td><%= track.album.artist.name %> </td>
    </tr>
    <% end %>
  </tbody>
</table>
