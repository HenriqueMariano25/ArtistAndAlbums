<h1>Albums</h1>

<table class="table table-striped table-bordered">
  <thead class="thead-dark">
    <tr>
      <th>Album</th>
      <th>Artist</th>
    </tr>
  </thead>
  <tbody>
    <% @albums.each do | album | %>
    <tr>
      <td><%= link_to album.title, album%></td>
      <td><%= album.artist.name %> </td>
    </tr>
    <% end %>
  </tbody>
</table>
